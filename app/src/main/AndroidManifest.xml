<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.mapview">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
                android:name=".MapLayerTestActivity"
                android:label="MapLayerTest">
        </activity>
        <activity
                android:name=".LocationLayerTestActivity"
                android:label="LocationLayerTest">
        </activity>
        <activity
                android:name=".BitmapLayerTestActivity"
                android:label="BitmapLayerTest">
        </activity>
        <activity
                android:name=".MarkLayerTestActivity"
                android:label="MarkLayerTest">
        </activity>
        <activity
                android:name=".RouteLayerTestActivity"
                android:label="RouteLayerTest">
        </activity>
    </application>
</manifest>